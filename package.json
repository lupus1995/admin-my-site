{
  "name": "blog",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --open --config webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:dev": "npx tsc --p server/tsconfig.json && tsc-alias -p server/tsconfig.json",
    "dev": "npm run build && npm run build:dev && nodemon -q server/dist/server/index.js --ignore '*.scss'",
    "test": "jest",
    "devr": "cross-env NODE_ENV=production webpack --config server/webpack.server.js && nodemon -q server/dist/app.js"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/**/*.spec.(ts|js|tsx|jsx)"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "jsx",
      "tsx"
    ],
    "moduleNameMapper": {
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^commons(.*)$": "<rootDir>/src/commons$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^pages(.*)$": "<rootDir>/src/pages$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ]
  },
  "author": "./dist/bundle.js",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.3.2",
